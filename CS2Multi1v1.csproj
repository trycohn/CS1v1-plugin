<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <!-- Исключить папку CounterStrikeSharp из сборки -->
  <ItemGroup>
    <Compile Remove="CounterStrikeSharp\**\*" />
    <Content Remove="CounterStrikeSharp\**\*" />
    <None Remove="CounterStrikeSharp\**\*" />
    <EmbeddedResource Remove="CounterStrikeSharp\**\*" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CounterStrikeSharp.API" Version="1.0.362" />
    <PackageReference Include="Google.Cloud.Logging.Log4Net" Version="4.1.0" />
  </ItemGroup>

</Project>
